        "darkaonline/l5-swagger": "^8.5",
        "diglactic/laravel-breadcrumbs": "^7.1",
        "fakerphp/faker": "^1.23",
        "genealabs/laravel-model-caching": "^0.11.7",
        "graham-campbell/github": "^12.4",
        "intervention/image": "^3.4",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/socialite": "^5.0",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "6.3.*",
        "marvinlabs/laravel-discord-logger": "^1.1",
        "nnoggie/mythicdungeontools": "4.3.3.5",
        "paquettg/php-html-parser-php-8-fix": "^3.1",
        "patreon/patreon": "^1.0.0",
        "phlak/semver": "^4.1",
        "predis/predis": "^1.1",
        "santigarcor/laratrust": "^7.1",
        "shrikeh/teapot": "^2.3",
        "socialiteproviders/battlenet": "^4.1.0",
        "socialiteproviders/discord": "^4.1.0",
        "spatie/url": "^1.3",
        "tray-labs/laravel-influxdb": "^1.0",
        "tremby/laravel-git-version": "^1.1",
        "yajra/laravel-datatables-oracle": "~9.0",
        "zircote/swagger-php": "^4.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "deployer/deployer": "^7.3",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0",
        "rector/rector": "^1.0",
        "wotuu/keystone.guru.deployer": "0.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true
        }
    }
}
