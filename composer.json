{
    "name": "wotuu/keystone.guru",
    "description": "A website where users can build and find their favorite routes for completing in Mythic Plus dungeons in World of WarcraftÂ®",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Wotuu/laravel4-datatables-package"
        },
        {
            "type": "package",
            "package": {
                "name": "wotuu/keystone.guru.deployer",
                "version": "0.1",
                "source": {
                    "url": "https://github.com/Wotuu/keystone.guru.deployer.git",
                    "type": "git",
                    "reference": "32383771f4f595edf52b7338e766faf33a4169f4"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "nnoggie/mythicdungeontools",
                "version": "4.3.3.5",
                "source": {
                    "url": "https://github.com/Wotuu/MythicDungeonTools.git",
                    "type": "git",
                    "reference": "90ca82cff5abf838775bd7b8ceb02c489faefe31"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "paquettg/php-html-parser-php-8-fix",
                "version": "3.1",
                "source": {
                    "url": "git@github.com:Wotuu/php-html-parser.git",
                    "type": "git",
                    "reference": "5d1c017c916f6ce8991442f1bead60729919ed8d"
                }
            }
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-lua": "*",
        "ext-zip": "*",
        "akaunting/laravel-language": "^1.0",
        "barryvdh/laravel-ide-helper": "^2.13",
