{
    "apiOriginAllow":{
        "allowCors" : true,
        "allowOrigin" : "LARAVEL_ECHO_ALLOW_ORIGIN",
        "allowMethods" : "GET, POST",
        "allowHeaders" : "Origin, Content-Type, X-Auth-Token, X-Requested-With, Accept, Authorization, X-CSRF-TOKEN, X-Socket-Id"
    },
    "authHost": "LARAVEL_ECHO_SERVER_AUTH_URL",
    "authEndpoint": "/broadcasting/auth",
    "clients": [
        {
            "appId": "LARAVEL_ECHO_SERVER_CLIENT_APP_ID",
            "key": "LARAVEL_ECHO_SERVER_CLIENT_KEY"
        }
    ],
    "database": "redis",
    "databaseConfig": {
        "redis": {
            "host": "REDIS_HOST",
            "port": "REDIS_PORT",
            "db": "REDIS_DB",
            "keyPrefix": "REDIS_PREFIX"
        },
        "sqlite": {
            "databasePath": "/database/laravel-echo-server.sqlite"
        },
        "publishPresence": true
    },
    "devMode": "APP_DEBUG",
    "host": "0.0.0.0",
    "port": 6001,
    "protocol": "http",
    "sslCertPath": "",
    "sslKeyPath": "",
    "sslCertChainPath": "",
    "sslPassphrase": "",
    "socketio": {},
    "subscribers": {
        "http": true,
        "redis": true
    }
}
